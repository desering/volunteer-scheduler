{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "styled-system": ["./styled-system"],
      "@payload-config": ["payload.config.ts"],
      "@payload-types": ["../shared/payload-types"],
      "~/*": ["./src/*"]
    },
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "types": ["./src/env.d.ts"]
  },
  "exclude": ["dist"]
}
