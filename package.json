{
  "name": "@desering/volunteer-scheduler",
  "version": "1.0.0",
  "description": "",
  "license": "AGPL-3.0-only",
  "type": "module",
  "scripts": {
    "dev": "panda codegen && next dev --port 3000 --turbopack",
    "devsafe": "rm -rf .next && next dev --turbopack",
    "build": "panda codegen && next build --turbopack",
    "check": "biome check --error-on-warnings",
    "check:staged": "biome check --error-on-warnings --staged --no-errors-on-unmatched",
    "lint:staged": "biome check --error-on-warnings --staged --no-errors-on-unmatched --write",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "payload": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload",
    "start": "next start",
    "generate": "bun run generate:importmap && bun run generate:types && bun run generate:panda",
    "generate:panda": "panda codegen",
    "generate:importmap": "payload generate:importmap",
    "generate:types": "payload generate:types",
    "generate:db-schema": "payload generate:db-schema",
    "prepare": "husky"
  },
  "dependencies": {
    "@ark-ui/react": "5.27.1",
    "@date-fns/tz": "1.4.1",
    "@date-fns/utc": "2.1.1",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/exporter-metrics-otlp-http": "^0.208.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.208.0",
    "@opentelemetry/instrumentation-http": "^0.208.0",
    "@opentelemetry/resources": "^2.2.0",
    "@opentelemetry/sdk-node": "^0.208.0",
    "@opentelemetry/semantic-conventions": "^1.38.0",
    "@payloadcms/db-postgres": "3.62.0",
    "@payloadcms/email-nodemailer": "3.62.0",
    "@payloadcms/next": "3.62.0",
    "@payloadcms/richtext-lexical": "3.62.0",
    "@payloadcms/ui": "3.62.0",
    "@react-email/components": "1.0.1",
    "@react-email/render": "2.0.0",
    "@tanstack/react-query": "5.90.7",
    "@types/canvas-confetti": "1.9.0",
    "@types/js-cookie": "3.0.6",
    "canvas-confetti": "1.9.4",
    "date-fns": "4.1.0",
    "dotenv": "16.4.7",
    "graphql": "16.12.0",
    "ical-generator": "10.0.0",
    "js-cookie": "3.0.5",
    "lucide-react": "0.556.0",
    "next": "15.5.9",
    "nodemailer": "7.0.11",
    "payload": "3.62.0",
    "pino": "9.14.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "sharp": "0.34.5",
    "typebox": "^1.0.49",
    "zod": "4.1.12"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.7",
    "@pandacss/dev": "^1.5.0",
    "@park-ui/panda-preset": "^0.43.1",
    "@types/bun": "1.3.7",
    "@types/node": "^24.10.0",
    "@types/nodemailer": "^7.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.2",
    "@vitest/coverage-v8": "4.0.13",
    "cross-env": "^10.1.0",
    "husky": "^9.1.7",
    "pino-pretty": "13.1.2",
    "typescript": "^5.9.3",
    "vitest": "^4.0.15"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0"
  }
}
